    {
        "keys": ["ctrl+alt+c"],
        "command": "set_file_type",
        "args": {"syntax": "Packages/C#/C#.sublime-syntax"}
    },
    {
        "keys": ["ctrl+alt+t"],
        "command": "set_file_type",
